{
  "name": "caikeal/coding-open-api",
  "description": "Coding Open Api SDK",
  "keywords": [
    "coding",
    "coding-sdk",
    "sdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "caikeal",
      "email": "caikeal@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.2",
    "monolog/monolog": "^1.22",
    "pimple/pimple": "^3.0",
    "symfony/cache": "^3.3|^4.0",
    "symfony/http-foundation": "^2.7|^3.0|^4.0",
    "symfony/psr-http-message-bridge": "^0.3|^1.0",
    "illuminate/events": "^5.6",
    "illuminate/queue": "^5.6",
    "illuminate/container": "^5.6",
    "illuminate/cache": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "overtrue/phplint": "dev-master",
    "mockery/mockery": "^1.0",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "CodingOpenApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodingOpenApi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CodingOpenApi\\LaravelCoding\\ServiceProvider"
      ],
      "aliases": {
        "CodingOpenApi": "CodingOpenApi\\LaravelCoding\\Facade"
      }
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    {
      "packagist": false
    }
  ]
}
